<div class="whiteBox" style="width: 216mm; height: 31.5mm; border: none; border-left: 2px solid; border-right: 2px solid; border-top: 2px solid;">
  <div class="whiteBox" style="width: 46mm; height: 23mm; border: none;">
    <img src="data:image/jpg;base64,{{logoImage}}" alt="logo" style="width:45mm; height:18mm; padding-left: 1mm; padding-top: 1mm;">
  </div>
  <div class="whiteBox" style="width: 122mm; height: 23mm; border: none;">
    <div class="whiteBox" style="width: 120mm; height: 5mm; border: none; font-size: 6mm; font-weight: bold; coor: black; line-height: 1.5;">Test Requisition</div>
    <div class="headerBox" style="width: 120mm; height: 5mm; border: none; font-size: 5mm; color: black;">Oncology Testing</div>
    <div class="headerBox" style="width: 120mm; height: 5mm; border: none; font-size: 5mm; color: black;">{{phone1}}</div>
    <div class="headerBox" style="width: 120mm; height: 5mm; border: none; font-size: 5mm; color: black;">{{fax}}</div>
  </div>
  <div class="whiteBox" style="width: 46mm; height: 23mm; border: none;">
    <img src="data:image/jpg;base64,{{neoImage}}" alt="neo" style="width:24.98mm; height:11.35mm; padding-left: 1mm; padding-top: 1mm;">
  </div>
  <div class="whiteBox" style="width: 46mm; height: 7mm; border: none;">
    <div class="headerBox" style="width: 46mm; height: 3.5mm; color: black; text-align: left; padding-left: 1mm;">{{address1line1}}</div>
    <div class="headerBox" style="width: 46mm; height: 3.5mm; color: black; text-align: left; padding-left: 1mm;">{{address1line2}}</div>
  </div>
  <div class="whiteBox" style="width: 120mm; height: 7mm; border: none;">
    {{#each pathologist}}
    <div class="headerBox" style="width: 40mm; height: 3mm; border: none; color: black; text-align: left;">{{this}}</div>
    {{/each}}
  </div>
  <div class="whiteBox" style="width: 46mm; height: 7mm; border: none;">
    <div class="headerBox" style="width: 46mm; height: 3.5mm; color: black; text-align: right;">{{address2line1}}</div>
    <div class="headerBox" style="width: 46mm; height: 3.5mm; color: black; text-align: right;">{{address2line2}}</div>
  </div>
</div>
