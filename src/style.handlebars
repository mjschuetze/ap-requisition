@media print {
  body {
    width: 8.5in;
    height: 11in;
    font-family: Calibri;
    -webkit-print-color-adjust: exact;
  }
  .checkBox {
    -webkit-print-color-adjust: exact;
  }
}
@media screen {
  body {
    width: 8.5in;
    height: 11in;
    font-family: Calibri;
  }
}
html,
body {
  background-color: #fff;
  margin: 0px;
}
@page {
  position: relative;
  display: block;
  background: #fff;
  page-break-after: auto;
  overflow: hidden;
  size: 8.5in 11in;
  padding-left: 1px;
  margin-top: 4mm;
  margin-left: 4mm;
  margin-right: 4mm;
  margin-bottom: 6mm;
}
.row {
  height: 6mm;
  width: 217mm;
}
.heading {
  position: relative;
  height: 6mm;
  width: 217mm;
  background: #a95346;
  font-weight: bold;
  text-align: center;
  letter-spacing: 0.1mm;
  color: #a95346;
  line-height: 1.5;
  font-size: 4.5mm;
}
.whiteBox {
  height: 6mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
}
.oncoBox {
  height: 4mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
}
.brownBox {
  height: 6mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
  background-color: #f9edef;
}
.yellowBox {
  height: 6mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
  background-color: #ffffb6;
}
.checkBox {
  width: 2mm;
  height: 2mm;
  background-color: #fff;
  position: relative;
  display: inline-block;
  border: 1px solid;
}
.underLine {
  height: 4mm;
  width: 68mm;
  position: relative;
  display: inline-block;
  float: left;
}
.highlight {
  background-color: #ffffb6;
}
