  <div class="whiteBox" style="width: 55mm; border: none;">
    <div class="underLine" style="width: 55mm; border-bottom: 1px solid"></div>
  </div>
  <div class="whiteBox" style="width: 15mm; border: none; border-right: 1px solid">{{phoneNumber}}</div>
  <div class="whiteBox" style="width: 20mm; border: none;">
    <div class="underLine" style="width: 20mm; border-bottom: 1px solid"></div>
  </div>
</div>
<div class="row">
  <div class="whiteBox" style="width: 10mm; border-bottom: none;">{{city}}</div>
  <div class="whiteBox" style="width: 40mm; border: none;">
    <div class="underLine" style="width: 40mm; border-bottom: 1px solid"></div>
  </div>
  <div class="whiteBox" style="width: 8mm; border: none;">{{state}}</div>
  <div class="whiteBox" style="width: 15mm; border: none;">
    <div class="underLine" style="width: 15mm; border-bottom: 1px solid"></div>
